var Welcome = function() {
	/**
	*	Private Methods (separated just by closing brace)
	*/

	return {
		/**
		*	Public Methods (separated by commas)
		*/
		documentReady: function() {
			$('#tv').prop('checked', true);
		}
	};
}();

$(document).ready(function() {
	Welcome.documentReady();
});
